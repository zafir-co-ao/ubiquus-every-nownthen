VENDUS_API_KEY=your-vendus-api-key
MTB_VENDUS_API_KEY=your-mtb-vendus-api-key

# ============================================================================
# Optional Configuration (defaults provided)
# ============================================================================

# Common
ROOT_FOLDER=1yStonR5SunFaBUPBCIw8WBzaw_dRWxph

